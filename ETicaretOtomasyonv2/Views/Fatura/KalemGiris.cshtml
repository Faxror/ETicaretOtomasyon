@model ETicaretOtomasyonv2.Models.Siniflar.FaturaKalem
@{
    ViewBag.Title = "KalemGiris";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<br />
<h2>Yeni Fatura Kalem Sayfası</h2>
<br />
<form class="form-group" method="post">

    @Html.Label("Açıklama")
    @Html.TextBoxFor(x => x.Açıklama, new { @class = "form-control" })
    <br />
    @Html.Label("Miktar")
    @Html.TextBoxFor(x => x.Miktar, new { @class = "form-control" })
    <br />
    @Html.Label("Birim Fiyat")
    @Html.TextBoxFor(x => x.BirtimFiyat, new { @class = "form-control" })
    <br />
    @Html.Label("Tutar")
    @Html.TextBoxFor(x => x.Tutar, new { @class = "form-control" })
    <br />
    @Html.Label("Fatura")
    @Html.TextBoxFor(x => x.FaturaID, new { @class = "form-control" })
    @*Html.TextBoxFor(x => x.Kategoriid, new { @class = "form-control" })*@
    <br />

    <button class="btn btn-info">Ekle</button>
</form>


